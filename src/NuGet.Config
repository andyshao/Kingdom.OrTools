<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <clear />
    <add key="globalPackagesFolder" value="packages" />
    <!-- TODO: TBD: ostensibly, may we use these defined bits as expanded arguments during the packageSources? -->
    <!-- TODO: TBD: Possible to expand custom config during packageSources / https://github.com/NuGet/Home/issues/8587 -->
    <!-- TODO: TBD: https://stackoverflow.com/questions/57927398/possible-to-use-nuget-config-custom-config -->
    <!-- TODO: TBD: i.e. "$ostoolsSatCodeGeneration$ $releasedPackagesLabel$", "$ostoolsSatCodeGeneration$.Attributes $releasedPackagesLabel$", ... -->
    <!-- TODO: TBD: i.e. "$ostoolsSatCodeGeneration$$binReleasePath", "$ostoolsSatCodeGeneration$.Attributes$binReleasePath", ... -->
    <add key="ostoolsSatCodeGeneration" value="Kingdom.OrTools.Sat.CodeGeneration" />
    <add key="releasedPackagesLabel" value="Released Packages" />
    <add key="binReleasePath" value="\bin\Release" />
  </config>
  <packageSources>
    <!--<add key="Internal Code Generation Packages" value="tools\NuGet\packages" />-->
    <add key="Kingdom.OrTools.Sat.CodeGeneration Released Packages" value="Kingdom.OrTools.Sat.CodeGeneration\bin\Release" />
    <add key="Kingdom.OrTools.Sat.CodeGeneration.Attributes Released Packages" value="Kingdom.OrTools.Sat.CodeGeneration.Attributes\bin\Release" />
    <add key="Kingdom.OrTools.Sat.CodeGeneration.Core Released Packages" value="Kingdom.OrTools.Sat.CodeGeneration.Core\bin\Release" />
    <add key="Kingdom.OrTools.Sat.CodeGeneration.Tasks Released Packages" value="Kingdom.OrTools.Sat.CodeGeneration.Tasks\bin\Release" />
    <add key="Kingdom.OrTools.Sat.CodeGeneration.Tool Released Packages" value="Kingdom.OrTools.Sat.CodeGeneration.Tool\bin\Release" />
  </packageSources>
</configuration>