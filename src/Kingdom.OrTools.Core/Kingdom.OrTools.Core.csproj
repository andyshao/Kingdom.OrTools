<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Configurations>Debug;Release</Configurations>
  </PropertyGroup>

  <PropertyGroup>
    <Description>Provides the core problem solver abstraction for use with Google OR-tools Constraint Solver and Linear Solver wrappers.</Description>
    <PackageTags>Google; Operations; Research; Tools; wrapper; disposable; CLR; object; host; problem; solver</PackageTags>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.17.1.4538</Version>
    <AssemblyVersion>2.17.1.4538</AssemblyVersion>
    <FileVersion>2.17.1.4538</FileVersion>
    <PackageVersion>21.17.1.4538</PackageVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Platform)'=='AnyCPU'">
    <DocumentationFile>bin\$(Configuration)\netstandard2.0\Kingdom.OrTools.Core.xml</DocumentationFile>
  </PropertyGroup>

  <ImportGroup>
    <Import Project="..\Licensing.props" />
  </ImportGroup>

</Project>
