<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Version>1.2.0.169</Version>
    <AssemblyVersion>1.2.0.169</AssemblyVersion>
    <FileVersion>1.2.0.169</FileVersion>
    <Configurations>Debug</Configurations>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Platform)' == 'AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Kingdom.OrTools.Sat.Parameters.Tests.csproj.DotSettings" />
  </ItemGroup>

  <!-- ReSharper disable UnknownProperty -->
  <ItemGroup>
    <PackageReference Include="xunit" Version="$(XunitVersion)" />
    <PackageReference Include="xunit.analyzers" Version="$(XunitAnalyzersVersion)" />
    <PackageReference Include="xunit.fluently.assert" Version="[1.3.1,2)" />
    <PackageReference Include="xunit.fluently.assert.ioly" Version="[1.2,2]" />
    <PackageReference Include="xunit.fluently.assert.exceptionally" Version="[1,2]" />
    <PackageReference Include="xunit.fluently.theoretically" Version="[1,2]" />
    <PackageReference Include="xunit.runner.visualstudio" Version="$(XunitVersion)">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  <!-- ReSharper restore UnknownProperty -->

  <!-- ReSharper disable UnknownProperty -->
  <ItemGroup>
    <PackageReference Include="Google.OrTools" Version="$(GoogleOrToolsVersion)" />
    <PackageReference Include="Google.Protobuf" Version="$(GoogleProtobufVersion)" />
  </ItemGroup>
  <!-- ReSharper restore UnknownProperty -->

  <ItemGroup>
    <PackageReference Include="Kingdom.OrTools.Sat.Parameters.Core" Version="*" />
    <PackageReference Include="Kingdom.OrTools.Sat.Parameters" Version="*" />
  </ItemGroup>

</Project>
